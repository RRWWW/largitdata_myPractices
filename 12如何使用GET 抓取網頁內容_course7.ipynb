      "<script>\r\n",
      "KISSY.use('node, cookie', function (S, N, cookie){\r\n",
      "  var $ = N.all;\r\n",
      "  \r\n",
      "  //獲取cookie\r\n",
      "  var hng = cookie.get('hng');\r\n",
      "  function getLocaleFromCookie() {\r\n",
      "    if (hng && hng.split('|').length == 3) {\r\n",
      "      return hng.split('|')[0];\r\n",
      "    } else {\r\n",
      "      return 'XX';\r\n",
      "    }\r\n",
      "  }\r\n",
      "  var locale = getLocaleFromCookie();\r\n",
